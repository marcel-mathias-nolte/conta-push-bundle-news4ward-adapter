{
    "name": "marcel-mathias-nolte/contao-push-bundle-news4ward-adapter",
    "type": "contao-bundle",
    "description": "Adapter to provide 'Push Bundle' functionality for the 'News4ward' bundle.",
    "homepage": "https://marcel.live",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Leo Feyer",
            "homepage": "https://github.com/leofeyer"
        },
        {
            "name": "Contao Community",
            "homepage": "https://contao.org/contributors"
        },
        {
            "name": "Marcel Mathias Nolte",
            "homepage": "https://marcel.live"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "^4.4",
        "contao-community-alliance/composer-plugin": "^2.4.0 || 3.*",
        "psi/news4ward": "~2",
        "dreibein/contao-push-bundle": "^1.0.5"
    },
    "require-dev": {
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2",
        "soundasleep/html2text": "~1.1"
    },
    "extra": {
        "contao-manager-plugin": "MarcelMathiasNolte\\ContaoPushBundleNews4wardAdapterBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "MarcelMathiasNolte\\ContaoPushBundleNews4wardAdapterBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcelMathiasNolte\\ContaoPushBundleNews4wardAdapterBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/marcel-mathias-nolte/contao-push-bundle-news4ward-adapter/issues",
        "source": "https://github.com/marcel-mathias-nolte/contao-push-bundle-news4ward-adapter"
    }
}
